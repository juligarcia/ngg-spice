{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "./resources/bjt/models_seed_data.txt": "models/bjt/models_seed_data.txt"
    }
  },
  "productName": "Graphic Spice",
  "version": "0.0.1",
  "identifier": "com.graphic.spice",
  "plugins": {},
  "app": {
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["$RESOURCE/**"]
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Graphic Spice",
        "width": 1920,
        "height": 1080,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
      }
    ]
  }
}
